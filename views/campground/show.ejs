<%include ../partials/header.ejs%>    <h3><%=campground.name%></h3>    <img src="<%=campground.image%>" alt="">    <p><%=campground.description%></p>    <p>        <a class="btn btn-success" href="/campgrounds/<%=campground._id%>/comments/new">Add New Comment</a>    </p>    <% campground.comments.forEach(function (comment) { %>        <p>        <stron><%= comment.author%></stron> - <%=comment.text%>        </p>    <%})%><%include ../partials/footer.ejs%>